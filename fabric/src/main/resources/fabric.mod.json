{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "BlayTheNinth"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}",
    "issues": "${issues}"
  },

  "license": "${license}",
  "icon": "${mod_id}.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.blay09.mods.craftingslots.fabric.FabricCraftingSlots"
    ],
    "client": [
      "net.blay09.mods.craftingslots.fabric.client.FabricCraftingSlotsClient"
    ],
    "fabric-datagen": [
      "net.blay09.mods.craftingslots.fabric.datagen.ModDataGenerator"
    ],
    "jei_mod_plugin": [
      "net.blay09.mods.craftingslots.addon.JEIAddon"
    ]
  },
  "mixins": [
    "craftingslots.mixins.json",
    "craftingslots.fabric.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "balm-fabric": "*",
    "minecraft": ">=${minimum_minecraft_version}",
    "java": ">=${java_version}"
  },
  "suggests": {
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/VAfZ2Nau6j"
      }
    }
  }
}
